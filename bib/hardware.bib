  doi = {10.1016/j.jmr.2015.11.017},
  author = {Kiryutin, Alexey S. and Pravdivtsev, Andrey N. and Ivanov, Konstantin L. and Grishin, Yuri A. and Vieth, Hans-Martin and Yurkovskaya, Alexandra V.},
  month = feb,
